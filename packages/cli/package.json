{
  "name": "@stackdock/cli",
  "version": "0.1.0",
  "private": true,
  "description": "StackDock CLI - Install components and adapters from the registry",
  "type": "module",
  "bin": {
    "stackdock": "./bin/stackdock.js"
  },
  "main": "./dist/index.js",
  "files": [
    "bin",
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "esbuild src/index.ts --bundle --platform=node --outfile=bin/stackdock.js --format=esm --external:fs --external:path --external:process",
    "dev": "tsx src/index.ts",
    "test": "vitest run",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "stackdock",
    "cli",
    "infrastructure",
    "registry"
  ],
  "author": "StackDock Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stackdock/stackdock.git",
    "directory": "packages/cli"
  },
  "dependencies": {
    "commander": "^12.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "esbuild": "^0.25.0",
    "tsx": "^4.7.0",
    "typescript": "^5.2.0",
    "vitest": "^3.0.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
